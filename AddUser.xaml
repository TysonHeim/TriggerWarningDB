<Window x:Class="d.AddUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:d"
        mc:Ignorable="d"
        Title="Add User" Height="257.2" Width="356.8">
    <Grid Margin="0,0,-6.6,0">
        <TextBox x:Name ="uName" HorizontalAlignment="Left" Height="23" Margin="36,86,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="287" />
        <TextBox x:Name ="pWord" HorizontalAlignment="Left" Height="23" Margin="36,115,0,0" TextWrapping="Wrap" Text="Password" VerticalAlignment="Top" Width="287" />
        <TextBox x:Name ="email" HorizontalAlignment="Left" Height="23" Margin="36,142,0,0" TextWrapping="Wrap" Text="Email" VerticalAlignment="Top" Width="287"/>
        <Button x:Name="create" Content="Create" HorizontalAlignment="Left" Margin="125,183,0,0" VerticalAlignment="Top" Width="75" Click="create_Click"/>
        <TextBox x:Name ="Last" HorizontalAlignment="Left" Height="23" Margin="36,58,0,0" TextWrapping="Wrap" Text="Last" VerticalAlignment="Top" Width="287" />
        <TextBox x:Name ="First" HorizontalAlignment="Left" Height="23" Margin="36,30,0,0" TextWrapping="Wrap" Text="First" VerticalAlignment="Top" Width="287" />
    </Grid>
</Window>
